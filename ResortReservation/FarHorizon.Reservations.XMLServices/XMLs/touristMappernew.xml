<?xml version="1.0" encoding="utf-8" ?>
<tourist>
  <table value="tblBookingTouristDetails"></table>
  <startRow value="2"></startRow>
  <readPattern value="columnwise" startRow="4" startcol="1" endcol="16">  
    <!--<mapper dbField="TouristNo" ExcelCell="1" ExcelFieldDescription="Sno."></mapper>-->
    <mapper dbField="RoomDetails" col="2" ExcelFieldDescription="Type-Single/Double/Twin/Trpl" Alpha="true"></mapper>
    <mapper dbField="Suffix" col="4" ExcelFieldDescription="Suffix" Alpha="true"></mapper>
    <mapper dbField="FirstName" col="5" ExcelFieldDescription="First Name" idField="true" Alpha="true" FetchTouristNo="true"></mapper>
    <mapper dbField="MiddleName" col="6" ExcelFieldDescription="Middle Name" idField="true" Alpha="true" FetchTouristNo="true"></mapper>
    <mapper dbField="LastName" col="7" ExcelFieldDescription="Last Name" idField="true" Alpha="true" FetchTouristNo="true"></mapper>
    <mapper dbField="Gender" col="8" ExcelFieldDescription="Gender" Alpha="true" FetchTouristNo="true"></mapper>
    <mapper dbField="NationalityId" col="9" ExcelFieldDescription="Nationality" MasterLookUpTable="tblNationalityMaster" MasterLookUpField="Nationality" MasterIdField="NationalityId" Alpha="false"></mapper>
    <mapper dbField="DOB" col="10" ExcelFieldDescription="Date of Birth" idField="true" Alpha="true" DateField="true"></mapper>
    <mapper dbField="PlaceofBirth" col="11" ExcelFieldDescription="Place of Birth" Alpha="true"></mapper>
    <mapper dbField="PassportNo" col="12" ExcelFieldDescription="Passport No." Alpha="true"></mapper>
    <mapper dbField="PPIssueDate" col="13" ExcelFieldDescription="Date of Issue of Passport" Alpha="true" DateField="true"></mapper>
    <mapper dbField="PPExpiryDate" col="14" ExcelFieldDescription="Date of Expiry of Passport" Alpha="true" DateField="true"></mapper>
    <mapper dbField="VisaNo" col="15" ExcelFieldDescription="Visa Number" Alpha="true"></mapper>
    <mapper dbField="VisaExpiryDate" col="16" ExcelFieldDescription="Date of Expiry of Visa" Alpha="true" DateField="true"></mapper>
  </readPattern>
  <readPattern value="rowwise" startRow="4" startcol="18"  endcol="18">
    <mapper dbField="IndiaEntryDate" row="4" col="18" ExcelFieldDescription="Date of Entry into India" Alpha="true" DateField="true"></mapper>
    <mapper dbField="ProposedStayInIndia" row="5" col="18" ExcelFieldDescription="Proposed stay in India" Alpha="true"></mapper>
    <mapper dbField="ArrivedFrom" row="6" col="18" ExcelFieldDescription="Arrived From" Alpha="true"></mapper>
    <mapper dbField="ArrivalDateTime" row="7" col="18" ExcelFieldDescription="Date of Arrival" Alpha="true" DateField="true"></mapper>
    <mapper dbField="ArrivalDateTime" row="8" col="18" ExcelFieldDescription="Time of Arrival" Alpha="true" TimeField="true" JoinToPrev="true"></mapper>
    <mapper dbField="TransportMode" row="9" col="18" ExcelFieldDescription="Mode of Transport" Alpha="true" ></mapper>
    <mapper dbField="VehicleName" row="10" col="18" ExcelFieldDescription="Vehicle Name/Type" Alpha="true"></mapper>
    <mapper dbField="VehicleNo" row="11" col="18" ExcelFieldDescription="Vehicle No." Alpha="true"></mapper>
    <mapper dbField="DriverName" row="12" col="18" ExcelFieldDescription="Drivers Name" Alpha="true"></mapper>
    <mapper dbField="DriverPhoneNo" row="13" col="18" ExcelFieldDescription="Drivers Phone No." Alpha="true"></mapper>
    <mapper dbField="TransportCompany" row="14" col="18" ExcelFieldDescription="Transport Company" Alpha="true"></mapper>
    <mapper dbField="TransportPhoneNo" row="15" col="18" ExcelFieldDescription="Transport Phone No." Alpha="true"></mapper>
    <mapper dbField="NextDestination" row="16" col="18" ExcelFieldDescription="Next Destination" Alpha="true"></mapper>
    <mapper dbField="DepartureDateTime" row="17" col="18" ExcelFieldDescription="Date of departure" Alpha="true" DateField="true"></mapper>
    <mapper dbField="DepartureDateTime" row="18" col="18" ExcelFieldDescription="Time of departure" Alpha="true" TimeField="true" JoinToPrev="true"></mapper>
    <mapper dbField="EmployedinIndia" row="19" col="18" ExcelFieldDescription="Employed In India" BoolField="true"></mapper>
    <mapper dbField="VisitPurpose" row="20" col="18" ExcelFieldDescription="Purpose of visit" Alpha="true"></mapper>
    <mapper dbField="PermanentAddressInIndia"></mapper>
    <mapper dbField="Allergies"></mapper>
    <mapper dbField="MealPref"></mapper>
    <mapper dbField="SpecialMessage"></mapper>
    <mapper dbField="MealPlan"></mapper>
  </readPattern>
</tourist>